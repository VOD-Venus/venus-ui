    "@nextui-org/theme": "^2.2.9",
    "clsx": "^2.1.1",
    "framer-motion": "^11.3.31",
    "immer": "^10.1.1",
    "modern-normalize": "^3.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.1",
    "zustand": "^4.5.5"
